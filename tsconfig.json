{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "electron/**/*", "Components/WriteOnImage.tsxxxx", "Components/UploadExcel.jsx", "Components/CertificateGenerator.jsx", "Components/ImageUploaderDB.jsx", "Components/ImageUploaderDB.jsx", "Components/ReadExcelParticipants.jsx", "Components/CertificateGeneratorExcel.jsx", "app/api/apiPdf/route.ts", "app/api/apiPdf/route.js", ],
  "exclude": ["node_modules"]
}
